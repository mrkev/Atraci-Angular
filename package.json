{
  "name": "Atraci",
  "version": "1.0.0",
  "description": "Free music streaming player",
  "repository": {
    "type": "git",
    "url": "http://github.com/Atraci/Atraci.git"
  },
  "main": "index.html",
  "window": {
    "width": 1096,
    "height": 700,
    "frame": false,
    "toolbar": true,
    "transparent": true,
    "icon": "assets/imgs/ico.png"
  },
  "single-instance": true,
  "dependencies": {
    "angular": "1.3.8",
    "angular-animate": "1.3.8",
    "angular-resource": "1.3.8",
    "angular-route": "1.3.8",
    "moment": "2.8.4",
    "nedb": "^1.1.1",
    "node-webkit-builder": "^1.0.6",
    "node-webkit-updater": "0.2.3",
    "request": "2.51.0",
    "underscore": "1.7.0",
    "youtube-api": "^0.3.2",
    "youtube-dl": "^1.8.0"
  },
  "devDependencies": {
    "gulp": "3.8.10",
    "gulp-util": "3.0.1",
    "gulp-ignore": "1.2.1",
    "gulp-sass": "1.2.4",
    "gulp-concat": "2.4.2",
    "gulp-shell": "0.2.11",
    "gulp-minify-css": "0.3.11"
  }
}
