{
  "name"        : "Atraci",
  "version"     : "1.0.0",
  "description" : "Free music streaming player",
  "repository"  : {
    "type"  : "git",
    "url"   : "http://github.com/Atraci/Atraci.git"
  },
  "main" : "index.html",
  "window": {
    "width": 1096,
    "height": 700,
    "frame": false,
    "toolbar": true
  },
  "single-instance": true,
  "dependencies" : {
    "angular"     : "1.3.8",
    "angular-resource" : "1.3.8",
    "angular-animate" : "1.3.8",
    "node-webkit-updater" : "0.2.3",
    "tarball-extract" : "0.0.3",
    "universal-analytics" : "0.3.6",
    "node-uuid"   : "1.4.0",
    "request"     : "2.51.0",
    "youtube-dl"  : "1.6.0",
    "gulp"        : "3.8.10",
    "gulp-sass" : "1.2.4",
    "gulp-concat" : "2.4.2",
    "gulp-shell" : "0.2.11",
    "gulp-minify-css" : "0.3.11"
  }
}